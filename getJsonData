func getJsonData(){
  let endpoint = "https://www.thecocktaildb.com/api/json/v1/1/random.php"
  let jsUrl:URL = URL(string: endpoint)!

  //magic step
  let jsonData = try? Data (contentsOf: jsUrl)
  //debug line
  print(jsonData ?? "Error: No Data to print. null value returned")


  //do stuff with it
  if (jsonData != nil){
    let dictionary:NSDictionary = (try! JSONSerialization.jsonObject(with: jsonData!, options: JSONSerialization.ReadingOptions.mutableContainers)) as! NSDictionary
    //debug
    print(dictionary)

    let drinkDictionary = dictionary["drinks"]! as! [[String:AnyObject]]

    for index in 0...drinkDictionary.count -1 {
      let singledrink = drinkDictionary[index]
      let drink = Menu()
      //do the following for each item
      drink.//information from json = singledrink["//name from class object"] as! string

      //finally append to the view controller array of drink objects
      Menu.append(drink)

    }
  }
}
